{
  "name": "polaris-dc/exact-online-laravel-client",
  "description": "EOL connector wrapper for the Laravel framework",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0|^9.0|^10.0|^11.0",
    "polaris-dc/exact-online-base-client": "^1.1"
  },
  "authors": [
    {
      "name": "Timothy De Cort",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "PolarisDC\\ExactOnline\\LaravelClient\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "PolarisDC\\ExactOnline\\LaravelClient\\ExactOnlineServiceProvider"
      ],
      "aliases": {
        "ExactOnlineClient": "PolarisDC\\ExactOnline\\LaravelClient\\Facades\\ExactOnlineClientFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^6.17"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/blobantwerp/exact-online-base-client.git"
    }
  ]
}

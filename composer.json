{
  "name": "polaris-dc/exact-online-laravel-client",
  "description": "Laravel package for Exact Online API connections.",
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0|^8.0",
    "polaris-dc/exact-online-base-client": "^1.1"
  },
  "authors": [
    {
      "name": "Maarten Waegeman",
      "email": "maarten.waegeman@polaris-dc.com",
      "role": "Developer"
    },
    {
      "name": "Timothy De Cort",
      "email": "Timothy.DeCort@polaris-dc.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "PolarisDC\\ExactOnline\\LaravelClient\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "PolarisDC\\ExactOnline\\LaravelClient\\ExactOnlineServiceProvider"
      ],
      "aliases": {
        "ExactOnlineClient": "PolarisDC\\ExactOnline\\LaravelClient\\Facades\\ExactOnlineClientFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^6.17"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.polaris-dc.app"
    }
  ]
}
